{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "user-registration-client": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/userregistrationclient",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": false,
            "assets": [
              "src/favicon.ico",
              "src/intro-img.svg",
              "src/assets"
            ],
            "styles": [
              "src/assets/vendor/bootstrap/css/bootstrap.min.css",
              "src/assets/vendor/icofont/icofont.min.css",
              "src/assets/vendor/boxicons/css/boxicons.min.css",
              "src/assets/vendor/remixicon/remixicon.css",
              "src/assets/vendor/venobox/venobox.css",
              "src/assets/vendor/owl.carousel/assets/owl.carousel.min.css",
              "src/assets/vendor/aos/aos.css",
              "src/assets/css/style.css",
              "node_modules/bootstrap/scss/bootstrap-grid.scss",
              "node_modules/bootstrap/scss/bootstrap-reboot.scss",
              "node_modules/bootstrap/scss/bootstrap.scss"
            ],
            "scripts": [
              "src/assets/vendor/jquery/jquery.min.js",
              "src/assets/vendor/bootstrap/js/bootstrap.bundle.min.js",
              "src/assets/vendor/php-email-form/validate.js",
              "src/app/lib/easing/easing.min.js",
              "src/assets/vendor/waypoints/jquery.waypoints.min.js",
              "src/app/lib/wow/wow.min.js",
              "src/assets/vendor/isotope-layout/isotope.pkgd.min.js",
              "src/assets/vendor/venobox/venobox.min.js",
              "src/assets/vendor/owl.carousel/owl.carousel.min.js",
              "src/assets/vendor/aos/aos.js",
              "src/app/lib/lightbox/js/lightbox.min.js",
              "src/contactform/contactform.js",
              "src/js/main.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "user-registration-client:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "user-registration-client:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "user-registration-client:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "user-registration-client:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "user-registration-client:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "user-registration-client",
  "cli": {
    "analytics": false
  }
}